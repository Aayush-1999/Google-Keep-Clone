<%include ./partials/header%>

<div class="container">
  <div class="form-width  mt-3">
    <div class="card animated zoomIn fast">
      <div class="card-content">
        <span class="card-title"><p class=" form-title">Reset Password</p></span>
        <form action="/resetpas/<%= token %>" method="POST" class="col s12">
            <div class="row">
                <div class="input-field col s12 ">
                    <input id="password" type="password" name="password" class="validate">
                    <label for="password">New Password</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12 ">
                    <input id="confirmpassword" type="password" name="confirm" class="validate">
                    <label for="confirmpassword">Confirm Password</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field center col s12">
                    <button class="waves-effect waves-light btn-large">
                        Submit
                    </button>
                </div>
            </div>
        </form>
      </div>
    </div>
  </div>
</div>


<% include ./partials/footer %>

<%include ./partials/footer%>